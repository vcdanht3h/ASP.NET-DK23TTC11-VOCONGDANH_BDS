@{
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <link href="~/css/stylelogin.css" rel="stylesheet" />
}
@model DoAnBds.Models.PhanQuyen
<br />
<table align="left" style="margin:0% 1%;">
    <tr>
        <td><a asp-controller="dashboard" asp-action="phanquyen" asp-area="admin"><img src="~/images/arrow.png" width="24" height="24" /></a></td>
    </tr>
</table>
<br />
<br />
<br />
<br />

<form class="kkk" align="center" method="post" asp-area="admin" asp-controller="dashboard" asp-action="addphanquyen">
    <h1 align="center" style="color:#003849;"><span>THÊM QUYỀN</span></h1>
    <div class="inset">
        <p>
            <label for="MaQuyen">Mã quyền</label>
            <input type="text" name="MaQuyen" id="MaQuyen" asp-for="MaQuyen">
        </p>
        <p>
            <label for="TenQuyen">Tên quyền</label>
            <input type="text" name="TenQuyen" id="TenQuyen" asp-for="TenQuyen">
        </p>
        <p>
            <label for="Username">Username</label>
            <input type="text" name="Username" id="Username" asp-for="Username">
        </p>
        <p>
            <label for="Password">Password</label>
            <input type="password" name="Password" id="Password" asp-for="Password">
        </p>
        <p>
            <label for="ChucVu">Chức vụ</label>
            <input type="text" name="ChucVu" id="ChucVu" asp-for="ChucVu">
        </p>
    </div>
    <p class="p-container">
        <input type="submit" name="go" id="go" value="Xác nhận">
    </p>
</form>